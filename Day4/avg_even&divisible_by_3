class Solution(object):
    def averageValue(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        l1=[]
        sum=0
        for i in nums:
            if(i%3==0 and i%2==0):
                l1.append(i)
        for j in l1:
            sum+=j
        if(len(l1)>0): 
            return sum/len(l1)
        else:
            return 0

        
